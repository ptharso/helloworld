<mvc:View controllerName="ns.helloworld.controller.App" xmlns="sap.m"	xmlns:mvc="sap.ui.core.mvc">
    <Page id="page" title="{i18n>homePageTitle}">
        <headerContent>
            <Button id="helly" icon="sap-icon://hello-world" class="sapUiSmallMarginEnd" text="{i18n>openDialogButtonText}"  press=".onOpenDialog"/>
        </headerContent>
        <content>
            <mvc:XMLView id="pview" viewName="ns.helloworld.view.Hellopanel"/>
            <mvc:XMLView id="lview" viewName="ns.helloworld.view.Invoicelist"/>
        </content>
    </Page>
</mvc:View>